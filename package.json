    "@nestjs/core": "^9.3.1",
    "@nestjs/microservices": "^9.0.5",
    "@nestjs/platform-express": "^9.0.0",
    "@padishah/toolbox": "^0.1.142",
    "@prisma/client": "^4.1.0",
    "class-validator": "^0.13.2",
    "grpc": "^1.24.11",
